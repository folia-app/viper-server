{
  "name": "viper-server",
  "watch": false,
  "env": {
    "PORT": 3001,
    "network": "homestead",
    "baseURL": "https://viper.folia.app/api",
    "NODE_ENV": "development",
    "GENERATE_GIFS": true,
    "SERVERS": 1
  },
  "script": "bin/www",
  "ignore_watch": [
    "node_modules",
    "output",
    "public"
  ],
  "watch_options": {
    "followSymlinks": false
  }
}