{
  "name": "viper-server-sepolia",
  "watch": true,
  "env": {
    "PORT": 3000,
    "network": "sepolia",
    "baseURL": "https://viper-sepolia.netlify.app/api",
    "NODE_ENV": "development",
    "GENERATE_GIFS": false,
    "SERVERS": 2
  },
  "script": "bin/www",
  "ignore_watch": [
    "node_modules",
    "output",
    "public"
  ],
  "watch_options": {
    "followSymlinks": false
  }
}